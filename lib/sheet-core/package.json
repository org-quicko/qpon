{
	"name": "@org-quicko/qpon-sheet-core",
	"version": "1.1.0",
	"description": "A library in typescript for common sheet entities of Qpon",
	"author": "Quicko <developer@quicko.org.in>",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.js",
	"types": "dist/types/src/index.d.ts",
	"type": "module",
	"scripts": {
		"clean": "npm cache clean --force",
		"prebuild": "rm -rf dist && npm run clean && eslint . && npm run generate",
		"lint": "eslint .",
		"build": "rollup -c",
		"generate": "schema-to-objects --input ./resources/schema/ --output ./generated"
	},
	"license": "UNLICENSED",
	"files": [
		"dist/*/**"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs",
			"types": "./dist/types/src/index.d.ts"
		},
		"./campaign_summary_workbook/beans": {
			"import": "./dist/esm/generated/campaign_summary_workbook/index.js",
			"require": "./dist/cjs/generated/campaign_summary_workbook/index.cjs",
			"types": "./dist/types/generated/campaign_summary_workbook/index.d.ts"
		},
		"./coupon_code_workbook/beans": {
			"import": "./dist/esm/generated/coupon_code_workbook/index.js",
			"require": "./dist/cjs/generated/coupon_code_workbook/index.cjs",
			"types": "./dist/types/generated/coupon_code_workbook/index.d.ts"
		},
		"./coupon_summary_workbook/beans": {
			"import": "./dist/esm/generated/coupon_summary_workbook/index.js",
			"require": "./dist/cjs/generated/coupon_summary_workbook/index.cjs",
			"types": "./dist/types/generated/coupon_summary_workbook/index.d.ts"
		},
		"./offer_workbook/beans": {
			"import": "./dist/esm/generated/offer_workbook/index.js",
			"require": "./dist/cjs/generated/offer_workbook/index.cjs",
			"types": "./dist/types/generated/offer_workbook/index.d.ts"
		},
		"./organization_summary_workbook/beans": {
			"import": "./dist/esm/generated/organization_summary_workbook/index.js",
			"require": "./dist/cjs/generated/organization_summary_workbook/index.cjs",
			"types": "./dist/types/generated/organization_summary_workbook/index.d.ts"
		},
		"./redemption_report_workbook/beans": {
			"import": "./dist/esm/generated/redemption_report_workbook/index.js",
			"require": "./dist/cjs/generated/redemption_report_workbook/index.cjs",
			"types": "./dist/types/generated/redemption_report_workbook/index.d.ts"
		},
		"./redemption_workbook/beans": {
			"import": "./dist/esm/generated/redemption_workbook/index.js",
			"require": "./dist/cjs/generated/redemption_workbook/index.cjs",
			"types": "./dist/types/generated/redemption_workbook/index.d.ts"
		},
		"./items_summary_workbook/beans": {
			"import": "./dist/esm/generated/items_summary_workbook/index.js",
			"require": "./dist/cjs/generated/items_summary_workbook/index.cjs",
			"types": "./dist/types/generated/items_summary_workbook/index.d.ts"
		},
		"./coupon_code_summary_workbook/beans": {
			"import": "./dist/esm/generated/coupon_code_summary_workbook/index.js",
			"require": "./dist/cjs/generated/coupon_code_summary_workbook/index.cjs",
			"types": "./dist/types/generated/coupon_code_summary_workbook/index.d.ts"
		},
		"./redemption_summary_workbook/beans": {
			"import": "./dist/esm/generated/redemption_summary_workbook/index.js",
			"require": "./dist/cjs/generated/redemption_summary_workbook/index.cjs",
			"types": "./dist/types/generated/redemption_summary_workbook/index.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"campaign_summary_workbook/beans": [
				"dist/types/generated/campaign_summary_workbook/index.d.ts"
			],
			"coupon_code_workbook/beans": [
				"dist/types/generated/coupon_code_workbook/index.d.ts"
			],
			"offer_workbook/beans": [
				"dist/types/generated/offer_workbook/index.d.ts"
			],
			"organization_summary_workbook/beans": [
				"dist/types/generated/organization_summary_workbook/index.d.ts"
			],
			"redemption_report_workbook/beans": [
				"dist/types/generated/redemption_report_workbook/index.d.ts"
			],
			"redemption_workbook/beans": [
				"dist/types/generated/redemption_workbook/index.d.ts"
			],
			"items_summary_workbook/beans": [
				"dist/types/generated/items_summary_workbook/index.d.ts"
			],
			"coupon_code_summary_workbook/beans": [
				"dist/types/generated/coupon_code_summary_workbook/index.d.ts"
			],
			"redemption_summary_workbook/beans": [
				"dist/types/generated/redemption_summary_workbook/index.d.ts"
			]
		}
	},
	"directories": {
		"lib": "./dist"
	},
	"devDependencies": {
		"@org-quicko/sheet-generator": "^1.0.1",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/node": "^22.7.0",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"esbuild-plugin-eslint": "^0.3.7",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-import-resolver-typescript": "^3.7.0",
		"glob": "^11.0.2",
		"rollup": "^4.41.1",
		"rollup-plugin-dts": "^6.2.1",
		"typescript": "^5.6.2"
	}
}